    "lint:fix": "standard --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/generation-s/jaures.git"
  },
  "keywords": [
    "microservice",
    "nodejs",
    "mongodb"
  ],
  "author": "",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/generation-s/jaures/issues"
  },
  "homepage": "https://github.com/generation-s/jaures#readme",
  "dependencies": {
    "dotenv": "^7.0.0",
    "knex": "^0.16.3",
    "mongodb": "^3.2.2",
    "pg": "^7.9.0",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "commitlint": "^7.5.2",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.10",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "yarnhook": "^0.4.0"
  },
  "standard": {
    "env": [
      "node",
      "jest",
      "serviceworker"
    ]
